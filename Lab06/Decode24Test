TD_Decode24Test_v.passTest ;
    %load/vec4 v0x558bc74ae0_0;
    %load/vec4 v0x558bc74bc0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 39 "$display", "%s passed", v0x558bc74d60_0 {0 0 0};
    %load/vec4 v0x558bc74ca0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x558bc74ca0_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 40 "$display", "%s failed: %d should be %d", v0x558bc74d60_0, v0x558bc74ae0_0, v0x558bc74bc0_0 {0 0 0};
T_1.3 ;
    %end;

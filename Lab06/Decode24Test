    %jmp T_2.4;
T_2.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x558bc75180_0, 0, 4;
    %jmp T_2.4;
T_2.4 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x558bc111b0;
T_3 ;

    %cmp/u;
    %jmp/1 T_2.3, 6;
    %jmp T_2.4;
T_2.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x558bc75180_0, 0, 4;
    %jmp T_2.4;
T_2.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x558bc75180_0, 0, 4;
    %jmp T_2.4;
T_2.2 ;

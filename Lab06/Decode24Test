v0x558bc74810_0 .var "passed", 7 0;
TD_Decode24Test_v.allPassed ;
    %load/vec4 v0x558bc74810_0;
    %load/vec4 v0x558bc4bb40_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 47 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 48 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
